																	<input type="text" id="sku" class="form-control round bg-transparent text-dark" placeholder="Enter Price">
																</fieldset>
															</div>
                                                             <div class="col-md-6">
																<label>IMEI</label>
																<fieldset class="form-group mb-3">
																	<input type="text" id="imei" class="form-control round bg-transparent text-dark" placeholder="Enter Price">
																</fieldset>
															</div>
                                                            <div class="col-md-6">
																<label>Quantity</label>
																<fieldset class="form-group mb-3">
                                                                    <input type="hidden" id="variationId" value="0">
																	<input type="number" id="quantity" class="form-control round bg-transparent text-dark" placeholder="Enter Price">
																</fieldset>
															</div>
                                                            <div class="col-md-6">
																<label>Selling Price</label>
																<fieldset class="form-group mb-3">
																	<input type="number" id="selling_price" class="form-control round bg-transparent text-dark" placeholder="Enter Price">
																</fieldset>
															</div>
															<div class="col-md-6">
																<label >Discount Price</label>
																<fieldset class="form-group mb-3">
																	<input type="number" id="discount_price" class="form-control round bg-transparent text-dark" placeholder="Enter Discount">
																</fieldset>
															</div>
														

														</div>
													</form>
													
														<table class="table table-bordered"	>
															<thead>
															  <tr class="row m-0 text-center">
																<th scope="col" class="col-3" >Variation</th>
																<th scope="col" class="col-2">Product(SKU)</th>
																<th scope="col" class="col-2">IMEI</th>
																<th scope="col" class="col-1">Quantity</th>
																<th scope="col" class="col-2">Price</th>
																<th scope="col" class="col-2">Sales Price</th>
															  </tr>
															</thead>
															<tbody id ="table-show" style="display: block;">
                                                                @foreach (var item in Model.ProductVariations)
                                                                {
                                                                     <tr class="row m-0 text-center" onclick="getData(
                                                                         '@item.Id','@item.Variation','@item.SKU','@item.IMEI',
                                                                         '@item.Quantity','@item.SellingPrice','@item.DiscountPrice'
                                                                     )">
                                                                        <td scope="col" class="col-3" >@item.Variation</td>
                                                                        <td scope="col" class="col-2">@item.SKU</td>
                                                                        <td scope="col" class="col-2">@item.IMEI</td>
                                                                        <td scope="col" class="col-1">@item.Quantity</td>
                                                                        <td scope="col" class="col-2">@item.SellingPrice</td>
                                                                        <td scope="col" class="col-2">@item.DiscountPrice</td>
                                                                    </tr>
                                                                }
