														<div class="form-group row">
															<div class="col-md-6">
																<label >Main Material</label>
																<fieldset class="form-group mb-3">
																	<input asp-for="ProductSpecification.MainMaterial" type="text" id="mainMaterial" class="form-control round bg-transparent text-dark" placeholder="Type Here">
																</fieldset>
															</div>
															<div class="col-md-6">
																<label >Color Family</label>
																<fieldset class="form-group mb-3">
																	<input type="text" asp-for="ProductSpecification.ColorFamily" id="colorFamily" class="form-control round bg-transparent text-dark" placeholder="Type Here">
																</fieldset>
															</div>
															<div class="col-md-6">
																<label >Product Dimension</label>
																<fieldset class="form-group mb-3">
																	<input type="text" asp-for="ProductSpecification.Dimension" id="dimension" class="form-control round bg-transparent text-dark" placeholder="Enter Weight">
																</fieldset>
															</div>
															<div class="col-md-6">
																<label >Product Weight</label>
																<fieldset class="form-group mb-3">
																	<input type="number" asp-for="ProductSpecification.Weight" id="weight" class="form-control round bg-transparent text-dark" placeholder="Enter Weight">
																</fieldset>
															</div>
															<div class="col-md-6">
																<label >Product Model</label>
																<fieldset class="form-group mb-3">
																	<input asp-for="ProductSpecification.Model" type="text" id="model" class="form-control round bg-transparent text-dark" placeholder="Type Here">
																</fieldset>
															</div>
