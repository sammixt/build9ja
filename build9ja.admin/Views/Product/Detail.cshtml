                                                                <div class="col-12 overflow-hidden">
                                                                    <textarea id="description" class="bg-transparent text-dark">
                                                                        @Model.ProductDescription
                                                                    </textarea>
                                                                </div>
                                                            </div>
															  <h6 class="text-body">
                                                                Additional Note
                                                            </h6>
                                                            <div class="row mb-3">
                                                                <div class="col-12 overflow-hidden">
                                                                    <textarea id="additional_note" class="bg-transparent text-dark">
                                                                        @Model.AdditionalNote
