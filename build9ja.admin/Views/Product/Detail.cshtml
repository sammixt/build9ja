																<label >Product Type</label>
																<fieldset class="form-group mb-3">
																	<input asp-for="ProductSpecification.ProductType" type="text" id="type" class="form-control round bg-transparent text-dark" placeholder="Type Here">
																</fieldset>
															</div>

														</div>
													</form>

												</div>
											
											</div>
