                                <h4>@product.ProductName</h4>
                                <ins>₦@product.BasePrice
                                  @* <del>
                                      $350.00     
                                    </del>  *@
                              </ins>
                              <div>
                                <a href="javascript:void(0)" onclick="addToCart('@product.ProductIdString')"><i class="fas fa-shopping-basket "></i></a>
                                <a href="javascript:void(0)" onclick="addToWishList('@product.ProductIdString')"><i class="fas fa-heart pl-1"></i></a>
