                  <nav aria-label="navigation">
                      <ul class="pagination">
                          
                          @{
                            
                            int totalPages = Model.PaginatedProducts.Count/Model.PaginatedProducts.PageSize;
                            int remainder = Model.PaginatedProducts.Count%Model.PaginatedProducts.PageSize;
                            int extraItems = remainder == 0 ? 0: 1;
                            totalPages = totalPages + extraItems;
                            string disableFirst = Model.PaginatedProducts.PageIndex == 1 ?"disabled":"";
                            string disableLast = Model.PaginatedProducts.PageIndex == totalPages ?"disabled":"";
                            <li class="page-item @disableFirst">
                              <a class="page-link page-link-prev" asp-action="Index" asp-controller="Shop" asp-route-category="@paths[3]"
                                asp-route-PageIndex="@(Model.PaginatedProducts.PageIndex - 1)" aria-label="Previous" tabindex="-1" aria-disabled="true">
                                  Prev
                              </a>
                          </li>
                            for(int i = 1;i <= totalPages; i++){
                              string active = i == Model.PaginatedProducts.PageIndex?"active":"";
                              <li class="page-item @active" aria-current="page">
                                <a class="page-link" asp-action="Index" asp-controller="Shop" asp-route-category="@paths[3]"
                                asp-route-PageIndex="@i">@i</a></li>
                            }
                            <li class="page-item @disableLast">
                              <a class="page-link page-link-next" asp-action="Index" asp-controller="Shop" asp-route-category="@paths[3]"
                                asp-route-PageIndex="@(Model.PaginatedProducts.PageIndex + 1)" aria-label="Next">
                                  Next
                              </a>
                          </li>
                          }
                          
                          @* <li class="page-item-total">of 6</li> *@
                          
                      </ul>
                  </nav>
